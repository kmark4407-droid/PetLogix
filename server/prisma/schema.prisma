generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Petlogix {
  id        Int      @id @default(autoincrement())
  name      String
  owner     String
  address   String?
  contact   String?
  species   String?
  breed     String?
  imageUrl  String?
  createdAt DateTime @default(now())
}
